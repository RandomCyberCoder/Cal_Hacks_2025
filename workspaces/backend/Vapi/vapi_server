import { VapiClient } from "@vapi-ai/server-sdk";
require("dotenv").config();

console.log("hi", process.env.VAPI_API_KEY);
const vapi = new VapiClient({ token: process.env.VAPI_API_KEY });

vapi.calls
  .create({
    assistantId: "6f62f7f1-7df6-4d57-bc30-4fafbf97506c", // ğŸ”¥ Use assistant from dashboard
    phoneNumberId: process.env.VAPI_PHONE_ID,
    customer: {
      number: "+9167087169",
    },
  })
  .then((res) => console.log("âœ… Call started:", res.id))
  .catch((err) =>
    console.error("âŒ Error:", err.response?.data || err.message)
  );
